digraph {

	ordering=out;
	ranksep=.4;
	bgcolor="lightgrey"; node [shape=box, fixedsize=false, fontsize=12, fontname="Helvetica-bold", fontcolor="blue"
		width=.25, height=.25, color="black", fillcolor="white", style="filled, solid, bold"];
	edge [arrowsize=.5, color="black", style="bold"]

  n0 [label="ROOT"];
  n1 [label="METHOD"];
  n1 [label="METHOD"];
  n2 [label="void"];
  n3 [label="main"];
  n4 [label="BLOCK"];
  n4 [label="BLOCK"];
  n5 [label="a"];
  n6 [label="="];
  n7 [label="-3"];
  n8 [label="*"];
  n9 [label="4"];
  n10 [label="/"];
  n11 [label="6"];
  n12 [label="+"];
  n13 [label="2"];
  n14 [label=";"];

  n0 -> n1 // "ROOT" -> "METHOD"
  n1 -> n2 // "METHOD" -> "void"
  n1 -> n3 // "METHOD" -> "main"
  n1 -> n4 // "METHOD" -> "BLOCK"
  n4 -> n5 // "BLOCK" -> "a"
  n4 -> n6 // "BLOCK" -> "="
  n4 -> n7 // "BLOCK" -> "-3"
  n4 -> n8 // "BLOCK" -> "*"
  n4 -> n9 // "BLOCK" -> "4"
  n4 -> n10 // "BLOCK" -> "/"
  n4 -> n11 // "BLOCK" -> "6"
  n4 -> n12 // "BLOCK" -> "+"
  n4 -> n13 // "BLOCK" -> "2"
  n4 -> n14 // "BLOCK" -> ";"

}

