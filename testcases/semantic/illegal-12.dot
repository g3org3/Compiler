digraph {

	ordering=out;
	ranksep=.4;
	bgcolor="lightgrey"; node [shape=box, fixedsize=false, fontsize=12, fontname="Helvetica-bold", fontcolor="blue"
		width=.25, height=.25, color="black", fillcolor="white", style="filled, solid, bold"];
	edge [arrowsize=.5, color="black", style="bold"]

  n0 [label="ROOT"];
  n1 [label="METHOD"];
  n1 [label="METHOD"];
  n2 [label="void"];
  n3 [label="main"];
  n4 [label="BLOCK"];
  n4 [label="BLOCK"];
  n5 [label="STATEMENT"];
  n5 [label="STATEMENT"];
  n6 [label="for"];
  n7 [label="x"];
  n8 [label="="];
  n9 [label="false"];
  n10 [label="10"];
  n11 [label="BLOCK"];

  n0 -> n1 // "ROOT" -> "METHOD"
  n1 -> n2 // "METHOD" -> "void"
  n1 -> n3 // "METHOD" -> "main"
  n1 -> n4 // "METHOD" -> "BLOCK"
  n4 -> n5 // "BLOCK" -> "STATEMENT"
  n5 -> n6 // "STATEMENT" -> "for"
  n5 -> n7 // "STATEMENT" -> "x"
  n5 -> n8 // "STATEMENT" -> "="
  n5 -> n9 // "STATEMENT" -> "false"
  n5 -> n10 // "STATEMENT" -> "10"
  n5 -> n11 // "STATEMENT" -> "BLOCK"

}

